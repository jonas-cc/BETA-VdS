<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".ui.room2.Room2Fragment"
    android:background="@drawable/textura">


    <com.denzcoskun.imageslider.ImageSlider
        android:id="@+id/SliderS2"
        android:layout_width="402dp"
        android:layout_height="192dp"
        android:layout_marginStart="-8dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="-8dp"
        app:iss_auto_cycle="true"
        app:iss_delay="0"
        app:iss_period="1000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textViewContenido2"
        android:layout_width="360dp"
        android:layout_height="53dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="sans-serif-black"
        android:text="Sabemos que tu evento debe ser inolvidable y te ofrecemos distintas modalidades.
        \n
        \n Te contamos que este salón es ideal para eventos infantiles!"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="240dp"
        android:layout_height="123dp"
        android:layout_marginTop="-26dp"
        android:src="@drawable/informacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SliderS2" />


    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="195dp"
        android:layout_height="106dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/servicios"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewContenido2" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="183dp"
        android:layout_height="48dp"
        android:layout_marginTop="-6dp"
        android:fontFamily="sans-serif-black"
        android:text="Si buscas economizar y tienes todo bajo control, podemos simplemente rentarte las instalaciones."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView23" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="183dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:fontFamily="sans-serif-black"
        android:text="Si tu intención es disfrutar tu evento al máximo y olvidarte de los pormenores, déjalo en nuestras manos y nosotros nos encargamos."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView25" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="192dp"
        android:layout_height="101dp"
        android:layout_marginTop="-19dp"
        android:src="@drawable/servicios1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="732dp"
        android:onClick="PR2"
        android:text="Paquetes"
        android:textSize="9sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="181dp"
        android:layout_height="104dp"
        android:layout_marginTop="-10dp"
        android:src="@drawable/servicios2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="147dp"
        android:layout_height="125dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/sa2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView23" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="195dp"
        android:layout_height="135dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="-1dp"
        android:src="@drawable/sa2_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.971"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView26" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="147dp"
        android:layout_height="89dp"
        android:layout_marginTop="36dp"
        android:src="@drawable/soli"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView17" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>